    "import pandas as pd\n",
    "X = [1,2,3,4]\n",
    "Y = [2,4,6,8]\n",
    "\n",
    "X = pd.DataFrame(X,columns=['X'])\n",
    "Y = pd.DataFrame(Y, columns=['Y'])\n",
    "\n",
    "from sklearn.linear_model import Ridge\n",
    "\n",
    "rd = Ridge(alpha=1)\n",
    "\n",
    "rd.fit(X, Y)\n",
    "\n",
    "print(rd.coef_)\n",
    "print(rd.intercept_)\n",
